{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
        {
          "src": "swagger.yaml",
          "use": "@vercel/static"
        }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    },
    { "src": "/swagger", "dest": "/swagger.yaml" }
  ]
}
