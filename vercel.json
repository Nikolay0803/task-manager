{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb",
        "memory": 1024,
        "timeout": 30
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    },
    { "src": "/api/docs", "dest": "/server.js" },
    { "src": "/api/docs/(.*)", "dest": "/server.js" },
    {
      "handle": "filesystem"
    }
  ]
}
